{
  "name": "@em/eslint-plugin-em",
  "version": "0.0.7",
  "description": "Sort styles alphabetically",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ether-Media/em-lint-rules.git"
  },
  "prettier": {
    "bracketSpacing": false,
    "bracketSameLine": false,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80,
    "endOfLine": "lf"
  },
  "author": "EtherMedia",
  "license": "Copyright",
  "devDependencies": {
    "prettier": "^3.0.3"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ]
}
